#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/yassn0/project/move-project/frontend/node_modules/.pnpm/gql.tada@1.9.0_graphql@16.12.0_typescript@5.9.3/node_modules/gql.tada/bin/node_modules:/home/yassn0/project/move-project/frontend/node_modules/.pnpm/gql.tada@1.9.0_graphql@16.12.0_typescript@5.9.3/node_modules/gql.tada/node_modules:/home/yassn0/project/move-project/frontend/node_modules/.pnpm/gql.tada@1.9.0_graphql@16.12.0_typescript@5.9.3/node_modules:/home/yassn0/project/move-project/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/yassn0/project/move-project/frontend/node_modules/.pnpm/gql.tada@1.9.0_graphql@16.12.0_typescript@5.9.3/node_modules/gql.tada/bin/node_modules:/home/yassn0/project/move-project/frontend/node_modules/.pnpm/gql.tada@1.9.0_graphql@16.12.0_typescript@5.9.3/node_modules/gql.tada/node_modules:/home/yassn0/project/move-project/frontend/node_modules/.pnpm/gql.tada@1.9.0_graphql@16.12.0_typescript@5.9.3/node_modules:/home/yassn0/project/move-project/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../gql.tada@1.9.0_graphql@16.12.0_typescript@5.9.3/node_modules/gql.tada/bin/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../gql.tada@1.9.0_graphql@16.12.0_typescript@5.9.3/node_modules/gql.tada/bin/cli.js" "$@"
fi
